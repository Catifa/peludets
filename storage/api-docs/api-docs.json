{
    "openapi": "3.0.0",
    "info": {
        "title": "API Usuarios",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost"
        }
    ],
    "paths": {
        "/api/usuario/userByProfOnly": {
            "post": {
                "summary": "Mostrar usuarios por profesion en el Home",
                "operationId": "App\\Http\\Controllers\\UsuariosController::userByProfOnly",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los usuarios por profesion."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/usuario/userByProf": {
            "post": {
                "summary": "Mostrar usuarios por profesion en Profesionales",
                "operationId": "App\\Http\\Controllers\\UsuariosController::searchByProf",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los usuarios por profesion."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/usuario/perfil": {
            "post": {
                "summary": "Recupera el id del usuario para mostrar el perfil que estas visitando",
                "operationId": "App\\Http\\Controllers\\UsuariosController::perfil",
                "responses": {
                    "200": {
                        "description": "Recupera el id del usuario para mostrar el perfil que estas visitando"
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}